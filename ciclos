#include "stdafx.h"
#include <iostream>
#include <time.h>
using namespace std;
void main() {
	int i,j,k,l;
	int t0;
	for (i=0;i<10;i++) {
		for (j=0;j<10;j++) {
			for (k=0;k<10;k++) {
				for (l=0;l<10;l++){
				cout<<"/r"<<i<<j<<k<<l;
				cout.flush();
				t0=time(0);
				while (t0==time(0)){}
			}
		}
	}
} 
